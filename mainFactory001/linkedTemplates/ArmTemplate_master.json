{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mainFactory001"
		},
		"Github_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Github'"
		},
		"Storage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Storage1'"
		},
		"mainproject_link_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'mainproject_link'"
		},
		"AmazonHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://media.githubusercontent.com/media/abdulmalik022/ecommerce-sales/main/"
		},
		"Github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mainproject.dfs.core.windows.net/"
		},
		"Repository_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"Storage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mainproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mainFactory001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Github_accountKey": {
						"value": "[parameters('Github_accountKey')]"
					},
					"Storage1_accountKey": {
						"value": "[parameters('Storage1_accountKey')]"
					},
					"mainproject_link_accessToken": {
						"value": "[parameters('mainproject_link_accessToken')]"
					},
					"AmazonHTTP_properties_typeProperties_url": {
						"value": "[parameters('AmazonHTTP_properties_typeProperties_url')]"
					},
					"Github_properties_typeProperties_url": {
						"value": "[parameters('Github_properties_typeProperties_url')]"
					},
					"Repository_properties_typeProperties_url": {
						"value": "[parameters('Repository_properties_typeProperties_url')]"
					},
					"Storage1_properties_typeProperties_url": {
						"value": "[parameters('Storage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}